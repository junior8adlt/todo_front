_TODOS = \"GET_TODOS\";\nconst SET_TODO = \"SET_TODO\";\nconst SET_TODOS = \"SET_TODOS\";\nconst CREATE_TODO = \"CREATE_TODO\";\nconst DELETE_TODO = \"DELETE_TODO\";\n//Reducers\nexport default function todoReducer(state = data, action) {\n  switch (action.type) {\n    case \"GET_TODOS\":\n      return {\n        ...state,\n        todos: action.payload,\n        originalTodos: action.payload,\n      };\n    case \"SET_TODO\":\n      return {\n        ...state,\n        todoSelected: action.payload,\n      };\n    case \"SET_TODOS\":\n      return {\n        ...state,\n        todos: action.payload,\n      };\n    case \"CREATE_TODO\":\n      const initialTodos = state.todos;\n      const origTodos = state.originalTodos;\n      return {\n        ...state,\n        todos: [...initialTodos, action.payload.data],\n        originalTodos: [...origTodos, action.payload.data],\n      };\n    case \"DELETE_TODO\":\n      const initialArray = state.todos;\n      const origArray = state.originalTodos;\n      const newInitialTodos = initialArray.filter(\n        (todo) => action.payload.data.id !== todo.id\n      );\n      const newOrigTodos = origArray.filter(\n        (todo) => action.payload.data.id !== todo.id\n      );\n      return {\n        ...state,\n        todos: newInitialTodos,\n        originalTodos: newOrigTodos,\n      };\n    default:\n      return state;\n  }\n}\n//Actions\n\nexport const getAllTodos = () => async (dispatch, getState) => {\n  try {\n    const resp = await axios.get(`http://localhost:4000/v1/todos`);\n    dispatch({\n      type: GET_TODOS,\n      payload: resp.data.todosResults,\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n\nexport const filterByDate = (date) => async (dispatch, getState) => {\n  try {\n    const todos = getState().todos.originalTodos;\n    let todosFilter = todos;\n    if (date) {\n      todosFilter = todos.filter(\n        (todo) => moment(todo.createdAt).format(\"YYYY-MM-DD\") === date\n      );\n    }\n    dispatch({\n      type: SET_TODOS,\n      payload: todosFilter,\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n\nexport const setSelectedTodo = (todo) => async (dispatch, getState) => {\n  try {\n    dispatch({\n      type: SET_TODO,\n      payload: todo,\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n\nexport const createTodo = (params, date) => async (dispatch, getState) => {\n  try {\n    const resp = await axios({\n      method: \"post\",\n      url: `http://localhost:4000/v1/todos`,\n      data: params,\n    });\n\n    dispatch({\n      type: CREATE_TODO,\n      payload: { data: resp.data.todo, date },\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n\nexport const updateCompleted = (todo) => async (dispatch, getState) => {\n  try {\n    const todos = getState().todos.todos;\n    await axios({\n      method: \"patch\",\n      url: `http://localhost:4000/v1/todos/${+todo.id}`,\n      data: {\n        completed: !todo.completed,\n      },\n    });\n    const editArray = todos.map((item) =>\n      item.id === todo.id\n        ? {\n            ...item,\n            completed: !todo.completed,\n          }\n        : item\n    );\n    dispatch({\n      type: SET_TODOS,\n      payload: editArray,\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n\nexport const updateTodo = (params) => async (dispatch, getState) => {\n  try {\n    const todos = getState().todos.todos;\n    const todoSelected = getState().todos.todoSelected;\n    await axios({\n      method: \"put\",\n      url: `http://localhost:4000/v1/todos/${+todoSelected.id}`,\n      data: params,\n    });\n    const editArray = todos.map((item) =>\n      item.id === todoSelected.id\n        ? {\n            ...item,\n            title: params.title,\n            description: params.description,\n          }\n        : item\n    );\n    dispatch({\n      type: SET_TODOS,\n      payload: editArray,\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n\nexport const deleteTodo = (todo) => async (dispatch, getState) => {\n  try {\n    const todos = getState().todos.todos;\n    await axios({\n      method: \"delete\",\n      url: `http://localhost:4000/v1/todos/${+todo.id}`,\n    });\n    dispatch({\n      type: DELETE_TODO,\n      payload: { data: todo },\n    });\n  } catch (error) {\n    console.log(error);\n  }\n};\n","/Users/albertojr/Desktop/todo_front/src/redux/generalDucks.js",[],"/Users/albertojr/Desktop/todo_front/src/components/Todos/Todos.jsx",["89"],"/Users/albertojr/Desktop/todo_front/src/hooks/useModal.js",[],"/Users/albertojr/Desktop/todo_front/src/components/CRUDModal.jsx",[],"/Users/albertojr/Desktop/todo_front/src/components/Sidebar/Sidebar.jsx",[],"/Users/albertojr/Desktop/todo_front/src/components/Modal/Modal.jsx",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\index.js",[],["90","91"],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\redux\\store.js",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\App.jsx",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\redux\\todoDucks.js",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\redux\\generalDucks.js",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\components\\Todos\\Todos.jsx",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\hooks\\useModal.js",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\components\\CRUDModal.jsx",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\components\\Sidebar\\Sidebar.jsx",[],"C:\\Users\\AlbertoJr\\Desktop\\todo_front\\src\\components\\Modal\\Modal.jsx",[],{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":176,"column":11,"nodeType":"98","messageId":"99","endLine":176,"endColumn":16},{"ruleId":"100","severity":1,"message":"101","line":28,"column":13,"nodeType":"102","endLine":37,"endColumn":4},{"ruleId":"92","replacedBy":"103"},{"ruleId":"94","replacedBy":"104"},"no-native-reassign",["105"],"no-negated-in-lhs",